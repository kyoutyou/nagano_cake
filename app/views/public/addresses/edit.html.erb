<div class="w-50 mx-auto">
  <h4>配送先編集</h4>
  <%= form_with model: @address do |f| %>
     <h6>郵便番号(ハイフンなし)</h6>
     <%= f.text_field :address %>
     <h6>住所</h6>
     <%= f.text_field :postal_code %>
     <h6>宛名</h6>
     <%= f.text_field :name %>
     <h6 class="m-5"><%= f.submit '変更内容を保存' %></h6>
  <% end %>
</div>